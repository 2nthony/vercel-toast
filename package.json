{
  "name": "vercel-toast",
  "description": "Display a brief toast at the browser",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "evillt/vercel-toast"
  },
  "author": "evillt <ijoec123@gmail.com> (https://evila.me)",
  "main": "dist/vercel-toast.min.js",
  "module": "dist/vercel-toast.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.d.ts",
    "dist/vercel-toast.esm.js",
    "dist/vercel-toast.js",
    "dist/vercel-toast.min.js",
    "dist/vercel-toast.css",
    "dist/vercel-toast.min.css"
  ],
  "scripts": {
    "test": "echo lol",
    "example": "poi --serve --config example/poi.config.js",
    "example:build": "poi --prod --config example/poi.config.js",
    "build": "bili",
    "docs": "typedoc src/index.ts --out example/public/docs --mode file --readme none --theme minimal --excludeNotExported",
    "now-build": "npm run docs && npm run example:build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@poi/plugin-typescript": "^12.3.1",
    "@saika/github-corner": "^0.1.0",
    "bili": "^4.8.1",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "poi": "^12.8.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.17.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "saika": "^2.12.3",
    "typedoc": "^0.15.0",
    "typescript": "^3.9.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuedown": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
